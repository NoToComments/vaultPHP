{
    "name": "notocommentsvault-php",
    "version": "2.0.0",
    "type": "library",
    "license": "MIT",
    "description": "This is a fork of mittwald/vault-php PHP library for Vault",
    "require": {
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.6",
        "php": "^7.0 || ^8.0",
        "php-http/httplug": ">=1.1.0"
    },
    "suggest": {
        "php-http/curl-client": "CURL Client Adapter",
        "tuupola/http-factory": "HTTP Factory"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0.0"
    },
    "authors": [
        {
            "name": "Marco Rieger",
            "email": "m.rieger@mittwald.de"
        },
        {
            "name": "Felix Franke",
            "email": "felixfranke95@gmx.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "VaultPHP\\": "src\\VaultPHP\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\VaultPHP\\": "tests\\VaultPHP\\"
        }
    },
    "scripts": {
        "test": "php ./vendor/bin/phpunit --configuration ./phpunit.xml.dist"
    }
}
